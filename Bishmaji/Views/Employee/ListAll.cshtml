@{

}
@model List<Bishmaji.Models.Employee>
@if (Model is null)
{

}
else
{
    <table class="table">
        <thead>
            <tr>
                <th scope="col">ID</th>
                <th scope="col">Name</th>
                <th scope="col">Salary</th>
                <th scope="col">Department</th>
                <th scope="col">Modify</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.EmployeeId</td>
                    <td>@item.EmployeeName</td>
                    <td>@item.Salary</td>
                    <td>@item.Department</td>
                    <td><button type="submit" class="btn btn-danger" asp-route-id="@item.EmployeeId">Delete</button></td>
                </tr>
            }


        </tbody>
    </table>
}
